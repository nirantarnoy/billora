<div class="p-10 space-y-10">
    <!-- Header & Search -->
    <div class="bg-white p-8 rounded-[2rem] shadow-sm border border-slate-100">
        <div class="flex flex-col md:flex-row md:items-center justify-between gap-6 mb-8">
            <div>
                <h2 class="text-3xl font-bold text-slate-800">รายงานรายได้และค่าธรรมเนียม</h2>
                <p class="text-slate-500 mt-1">สรุปการเงินแยกตามช่องทางแพลตฟอร์ม</p>
            </div>
            <div class="flex items-center gap-3">
                <button
                    class="flex items-center gap-2 px-6 py-3 bg-emerald-50 text-emerald-600 rounded-2xl font-bold hover:bg-emerald-100 transition-all">
                    <i class="fas fa-file-excel"></i> Export Excel
                </button>
                <button
                    class="flex items-center gap-2 px-6 py-3 bg-rose-50 text-rose-600 rounded-2xl font-bold hover:bg-rose-100 transition-all">
                    <i class="fas fa-file-pdf"></i> Export PDF
                </button>
            </div>
        </div>

        <form method="GET" class="grid grid-cols-1 md:grid-cols-4 lg:grid-cols-5 gap-6 items-end">
            <div>
                <label
                    class="block text-[10px] font-bold text-slate-400 uppercase tracking-widest mb-2 ml-1">เลขคำสั่งซื้อ</label>
                <input type="text" name="orderId" value="<%= query.orderId || '' %>"
                    placeholder="ระบุเลขคำสั่งซื้อ (ถ้ามี)"
                    class="w-full bg-slate-50 border border-slate-100 rounded-xl px-4 py-3 focus:ring-2 focus:ring-emerald-500 outline-none transition-all text-sm font-semibold">
            </div>
            <div>
                <label
                    class="block text-[10px] font-bold text-slate-400 uppercase tracking-widest mb-2 ml-1">เริ่มวันที่</label>
                <input type="date" name="dateFrom" value="<%= query.dateFrom %>"
                    class="w-full bg-slate-50 border border-slate-100 rounded-xl px-4 py-3 focus:ring-2 focus:ring-emerald-500 outline-none transition-all text-sm font-semibold">
            </div>
            <div>
                <label
                    class="block text-[10px] font-bold text-slate-400 uppercase tracking-widest mb-2 ml-1">ถึงวันที่</label>
                <input type="date" name="dateTo" value="<%= query.dateTo %>"
                    class="w-full bg-slate-50 border border-slate-100 rounded-xl px-4 py-3 focus:ring-2 focus:ring-emerald-500 outline-none transition-all text-sm font-semibold">
            </div>
            <div>
                <label
                    class="block text-[10px] font-bold text-slate-400 uppercase tracking-widest mb-2 ml-1">ช่องทาง</label>
                <select name="channel"
                    class="w-full bg-slate-50 border border-slate-100 rounded-xl px-4 py-3 focus:ring-2 focus:ring-emerald-500 outline-none transition-all text-sm font-semibold">
                    <option value="">ทั้งหมด</option>
                    <% channels.forEach(ch=> { %>
                        <option value="<%= ch.id %>" <%=query.channel==ch.id ? 'selected' : '' %>><%= ch.name %>
                        </option>
                        <% }) %>
                </select>
            </div>
            <div class="flex gap-2">
                <button type="submit"
                    class="flex-1 bg-slate-800 hover:bg-slate-900 text-white px-6 py-3 rounded-xl font-bold transition-all text-sm flex items-center justify-center gap-2">
                    <i class="fas fa-search"></i> ค้นหา
                </button>
                <a href="/fee-report"
                    class="bg-slate-100 p-3 rounded-xl text-slate-500 hover:bg-slate-200 transition-all flex items-center justify-center">
                    <i class="fas fa-undo"></i>
                </a>
            </div>
        </form>
    </div>

    <!-- Summary Section -->
    <% const hasShopee=items.some(i=> i.channel_name === 'Shopee');
        const hasTiktok = items.some(i => i.channel_name === 'Tiktok');
