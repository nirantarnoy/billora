                                            <th class="px-6 py-5">แพลตฟอร์ม</th>
                                            <th class="px-6 py-5 text-right">ยอดรวม</th>
                                            <th class="px-6 py-5 text-right">ค่าธรรมเนียม</th>
                                            <th class="px-8 py-5 text-right">สุทธิ</th>
                                        </tr>
                                    </thead>
                                    <tbody class="divide-y divide-slate-50">
                                        <% items.forEach(item=> { %>
                                            <tr class="hover:bg-slate-50/50 transition-all">
                                                <td class="px-8 py-5 text-sm"><span class="font-bold text-slate-700">
                                                        <%= new Date(item.order_date).toLocaleDateString('th-TH') %>
                                                    </span><span class="block text-[10px] text-slate-400">
                                                        <%= new Date(item.order_date).toLocaleTimeString('th-TH') %>
                                                    </span></td>
                                                <td class="px-6 py-5 text-sm font-bold text-slate-800">#<%=
                                                        item.order_id || item.order_sn %>
                                                </td>
                                                <td class="px-6 py-5">
                                                    <div class="flex items-center gap-2">
                                                        <% if (item.channel_name==='Shopee' ) { %>
                                                            <div
                                                                class="w-6 h-6 rounded bg-orange-500 text-white flex items-center justify-center text-[10px] font-black">
                                                                S</div>
                                                            <% } else if (item.channel_name==='Tiktok' ) { %>
                                                                <div
                                                                    class="w-6 h-6 rounded bg-black text-white flex items-center justify-center text-[10px] font-black">
                                                                    T</div>
                                                                <% } %><span class="text-xs font-bold text-slate-600">
                                                                        <%= item.channel_name %>
                                                                    </span>
                                                    </div>
                                                </td>
                                                <td class="px-6 py-5 text-right text-sm font-bold text-slate-700">฿<%=
                                                        item.total_amount.toLocaleString(undefined,
                                                        {minimumFractionDigits: 2}) %>
                                                </td>
                                                <% const fees=item.channel_name==='Shopee' ? (parseFloat(item.s_comm) +
                                                    parseFloat(item.s_trans) + parseFloat(item.s_service)) :
                                                    parseFloat(item.t_settlement - item.t_revenue) || 0; %>
                                                    <td class="px-6 py-5 text-right text-sm font-bold text-rose-500">-฿
                                                        <%= Math.abs(fees || 0).toLocaleString(undefined,
                                                            {minimumFractionDigits: 2}) %>
                                                    </td>
                                                    <td
                                                        class="px-8 py-5 text-right text-sm font-black text-emerald-600">
                                                        ฿<%= (item.channel_name==='Shopee' ? item.s_escrow :
                                                            item.t_settlement).toLocaleString(undefined,
                                                            {minimumFractionDigits: 2}) %>
                                                    </td>
                                            </tr>
                                            <% }) %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
</div>